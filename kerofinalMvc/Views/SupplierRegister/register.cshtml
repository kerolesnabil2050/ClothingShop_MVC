    <link rel="stylesheet" href="vendor/nouislider/nouislider.min.css">

    <!-- Main css -->
    <link rel="stylesheet" href="/css/styleG.css">
</head>
<body class="w-75">

    <div class="main container w-75">

        <div class="container w-75">
            <div class="signup-content">
                <div class="signup-img">
                    <img src="/img/Log2.jpg" alt="">
                    <div class="signup-img-content">
                        <h2>Register now </h2>
                        <p>while seats are available !</p>
                    </div>
                </div>
                <div class="signup-form">
                    <form class="register-form" asp-action="register" enctype="multipart/form-data" id="register-form" >
                        <div asp-validation-summary="All" class="text-danger"></div>
                        <div class="form-row">
                            <div class="form-group">
                                <div class="form-input">
                                    <label asp-for="FirstName"></label>
                                    <input asp-for="FirstName" />
                                    <span asp-validation-for="FirstName" class="text-danger"></span>
                                </div>
                                <div class="form-input">
                                    <label asp-for="LastName"></label>
                                    <input asp-for="LastName" />
                                    <span asp-validation-for="LastName" class="text-danger"></span>
                                </div>
                                <div class="form-input">
                                    <label asp-for="PassWord"></label>
                                    <input asp-for="PassWord" />
                                    <span asp-validation-for="PassWord" class="text-danger"></span>
                                </div>
                                <div class="form-input">
                                    <label asp-for="ConfirmPassword"></label>
                                    <input asp-for="ConfirmPassword" />
                                    <span asp-validation-for="ConfirmPassword" class="text-danger"></span>


                                </div>
                                <div class="form-input">
                                    <label asp-for="Email"></label>
                                    <input asp-for="Email" />
                                    <span asp-validation-for="Email" class="text-danger"></span>

                                </div>

                                <div class="form-input">
                                    <label asp-for="PhoneNumber"></label>
                                    <input asp-for="PhoneNumber" />
                                    <span asp-validation-for="PhoneNumber" class="text-danger"></span>

                                </div>
                            </div>
                            <div class="form-group">
                                <div>
                                    <label asp-for="Adress"></label>
                                    <input asp-for="Adress" />
                                    <span asp-validation-for="Adress" class="text-danger"></span>

                                </div>


                                <div class="form-input">
                                    <label asp-for="SSD"></label>
                                    <input asp-for="SSD">
                                    <span asp-validation-for="SSD" class="text-danger"></span>
                                </div>
                                <div class="form-input">
                                    <label asp-for="UserName"></label>
                                    <input asp-for="UserName" id="demand">
                                    <span asp-validation-for="UserName" class="text-danger"></span>

                                </div>
                              
                                <div class="form-input">
                                    <label asp-for="Photo"></label>
                                    <input asp-for="Photo" type="file" id="blank_name" />
                                    <span asp-validation-for="Photo" class="text-danger"></span>

                                </div>
                                <div class="form-input">
                                    <label asp-for="Age"></label>
                                    <input asp-for="Age" id="payable" />
                                    <span asp-validation-for="Age" class="text-danger"></span>
                                </div>
                            </div>
                        </div>

                        <div class="form-submit mt-5">
                            <input type="submit" value="Submit" class="submit" id="submit" name="submit" style="background-color:black" />
                        </div>
                    </form>
                </div>
            </div>
        </div>

    </div>

    <!-- JS -->
    <script src="/js/vendor/jquery/jquery.min.js"></script>
    <script src="/js/vendor/nouislider/nouislider.min.js"></script>
    <script src="/js/vendor/wnumb/wNumb.js"></script>

